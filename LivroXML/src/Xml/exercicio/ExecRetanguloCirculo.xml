<!DOCTYPE figure [
        <!ELEMENT figure (rectangle*,circle*)>
        <!ELEMENT rectangle (#PCDATA)>
        <!ATTLIST rectangle x CDATA #REQUIRED
                y CDATA #REQUIRED
                width CDATA #REQUIRED
                heigth CDATA #REQUIRED>
        <!ELEMENT circle (#PCDATA)>
        <!ATTLIST circle x CDATA #REQUIRED
                y CDATA #REQUIRED
                radius CDATA #REQUIRED>
        ]>

<figure>
    <rectangle x="50" y="50" width="100" heigth="100"></rectangle>
    <circle x="300" y="250" radius="200"></circle>
</figure>
